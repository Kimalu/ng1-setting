  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "devs": "webpack-dev-server --content-base build --hot --inline --devtool false",
    "build": "cnpm install && webpack --progress --prod",
    "test:unit": "karma start build/karma.unit.config.js",
    "test:cover": "karma start build/karma.cover.config.js",
    "lint": "eslint src build test",
    "test": "rm -rf ./source/test/unit/results && karma start ./karma.config.js",
    "codecov": "cat ./source/test/unit/results/coverage/lcov.info | codecov",
    "test:watch": "karma start ./karma.config.js --watch",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "e2e": "protractor ./protractor.config.js",
    "server": "nohup http-server ./build &"
  },
  "author": {
    "name": "linh",
    "email": "linhao354@gmail.com"
  },
